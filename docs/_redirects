# Cloudflare Pages Redirects for subdomains

# Root domain serves from root
/ /index.html 200

# API subdomain
/api/* /api/:splat 200

# Blog subdomain  
/blog/* /blog/:splat 200

# Community subdomain
/community/* /community/:splat 200

# Dashboard subdomain
/dashboard/* /dashboard/:splat 200

# Docs subdomain
/docs/* /docs/:splat 200

# Explorer subdomain
/explorer/* /explorer/:splat 200

# Playground subdomain
/playground/* /playground/:splat 200

# QuicPair subdomain
/quicpair/* /quicpair/:splat 200

# Security subdomain
/security/* /security/:splat 200

# Status subdomain
/status/* /status/:splat 200